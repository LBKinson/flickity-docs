    <div class="gallery gallery--as-nav-for-nav gallery--nav js-flickity"
      data-flickity-options='{ "asNavFor": ".gallery--as-nav-for-main", "contain": true, "pageDots": false }'>
      <div class="gallery-cell"></div>
      <div class="gallery-cell"></div>
