<div class="arrow-wiz" data-js-module="arrow-wiz">

  <div class="arrow-wiz-illo">
    <span class="arrow-wiz-illo__graph-label
      arrow-wiz-illo__graph-label--y arrow-wiz-illo__graph-label--y-pos50">50</span>
    <span class="arrow-wiz-illo__graph-label
      arrow-wiz-illo__graph-label--y arrow-wiz-illo__graph-label--y-0">0</span>
    <span class="arrow-wiz-illo__graph-label
      arrow-wiz-illo__graph-label--y arrow-wiz-illo__graph-label--y-neg50">-50</span>
    <span class="arrow-wiz-illo__graph-label
      arrow-wiz-illo__graph-label--x arrow-wiz-illo__graph-label--x-0">0</span>
    <span class="arrow-wiz-illo__graph-label
      arrow-wiz-illo__graph-label--x arrow-wiz-illo__graph-label--x-50">50</span>
    <span class="arrow-wiz-illo__graph-label
      arrow-wiz-illo__graph-label--x arrow-wiz-illo__graph-label--x-100">100</span>

    <canvas class="arrow-wiz-illo__canvas" width="200" height="200"></canvas>

    <div class="arrow-wiz-illo__top-well">
      <div class="arrow-wiz-illo__point arrow-wiz-illo__point--1" data-point="1">
        <div class="arrow-wiz-illo__point__dot"></div>
        <div class="arrow-wiz-illo__point__label">p1</div>
      </div>
      <div class="arrow-wiz-illo__point arrow-wiz-illo__point--2" data-point="2">
        <div class="arrow-wiz-illo__point__dot"></div>
        <div class="arrow-wiz-illo__point__label">p2</div>
      </div>
    </div>

    <div class="arrow-wiz-illo__center-well">
      <div class="arrow-wiz-illo__point arrow-wiz-illo__point--0" data-point="0">
        <div class="arrow-wiz-illo__point__dot"></div>
        <div class="arrow-wiz-illo__point__label">p0</div>
      </div>
      <div class="arrow-wiz-illo__point arrow-wiz-illo__point--3" data-point="3">
        <div class="arrow-wiz-illo__point__dot"></div>
        <div class="arrow-wiz-illo__point__label">p3</div>
      </div>
    </div>

  </div>

  <pre class="arrow-wiz-code"><code></code></pre>

  <div class="gallery">
    <div class="gallery-cell"></div>
    <div class="gallery-cell"></div>
    <div class="gallery-cell"></div>
    <div class="gallery-cell"></div>
    <div class="gallery-cell"></div>
  </div>

</div>